    "@tsed/engines": "^7.42.0",
    "@tsed/exceptions": "^7.42.0",
    "@tsed/json-mapper": "^7.42.0",
    "@tsed/logger": "^6.6.3",
    "@tsed/logger-file": "^6.6.3",
    "@tsed/openspec": "^7.42.0",
    "@tsed/platform-cache": "^7.42.0",
    "@tsed/platform-exceptions": "^7.42.0",
    "@tsed/platform-express": "^7.42.0",
    "@tsed/platform-log-middleware": "^7.42.0",
    "@tsed/platform-middlewares": "^7.42.0",
    "@tsed/platform-params": "^7.42.0",
    "@tsed/platform-response-filter": "^7.42.0",
    "@tsed/platform-serverless": "^7.42.0",
    "@tsed/platform-serverless-http": "^7.42.0",
    "@tsed/platform-serverless-testing": "^7.42.0",
    "@tsed/platform-views": "^7.42.0",
    "@tsed/prisma": "^7.42.0",
    "@tsed/schema": "^7.42.0",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "barrelsby": "^2.8.1",
